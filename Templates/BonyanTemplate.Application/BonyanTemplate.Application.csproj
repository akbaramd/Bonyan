        <ProjectReference Include="..\..\Framework\Src\Bonyan.AspNetCore\Bonyan.AspNetCore.csproj"/>
        <ProjectReference Include="..\..\Framework\Src\Bonyan.AutoMapper\Bonyan.AutoMapper.csproj"/>
        <ProjectReference Include="..\..\Modules\Bonyan.IdentityManagement\Src\Bonyan.Module.IdentityManagement.Application\Bonyan.Module.IdentityManagement.Application.csproj" />
        <ProjectReference Include="..\..\Modules\Bonyan.TenantManagement\Src\Bonyan.Module.TenantManagement.Application\Bonyan.Module.TenantManagement.Application.csproj" />
        <ProjectReference Include="..\BonyanTemplate.Domain\BonyanTemplate.Domain.csproj"/>
    </ItemGroup>

</Project>
